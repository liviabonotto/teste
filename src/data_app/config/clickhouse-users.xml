<yandex>
  <users>
    <default>
      <password>admin</password>
      <networks>
        <ip>::/0</ip>
      </networks>
      <profile>default</profile>
      <quota>default</quota>
    </default>
  </users>

  <profiles>
    <default>
        <max_memory_usage>15000000000000</max_memory_usage>
        <max_bytes_before_external_group_by>5000000000</max_bytes_before_external_group_by> 
        <max_bytes_before_external_sort>5000000000</max_bytes_before_external_sort> 
        <max_block_size>65536</max_block_size>
        <!-- <max_bytes_before_external_join>5000000000</max_bytes_before_external_join> -->
    </default>
  </profiles>

  <quotas>
    <default>
      <interval>
        <duration>3600</duration>
        <queries>0</queries>
        <errors>0</errors>
        <result_rows>0</result_rows>
        <read_rows>0</read_rows>
        <execution_time>0</execution_time>
      </interval>
    </default>
  </quotas>
</yandex>